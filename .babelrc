{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "ie": 11,
          "safari": 9,
          "browsers": "last 2 versions"
        }
      }
    ],
    "stage-2"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "stage-2"
      ]
    }
  }
}
